<!-- DataTales Example -->
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Data Verified</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
    
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>ID TRANSAKSI</th>
                        <th>JENIS_TRANSAKSI</th>
                        <th>JUMLAH_TRANSAKSI</th>                                                                       
                        <th>TANGGAL</th>                                                        
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>ID TRANSAKSI</th>
                        <th>JENIS_TRANSAKSI</th>
                        <th>JUMLAH_TRANSAKSI</th>                                                        
                        <th>TANGGAL</th>                                                        
                    </tr>
                </tfoot>
                <tbody>                    
                    <% temp.forEach(dt => { %>
                        <tr>
                            <td><%- dt.IDTDETAIL %> </td>
                            <td><%- dt.JENIS_TRANSAKSI %></td>
                            <td><%- dt.JUMLAH_TRANSAKSI %></td>
                            <td><%- dt.TGL_TRANSAKSI %></td>
                        </tr>            
                        <% }) %>
                </tbody>
            </table>
        </div>
    </div>
</div>